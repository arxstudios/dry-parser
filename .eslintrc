{
	"env": {
		"node": true,
		"mocha": true
	},
	"globals": {
		"angular"
	},
	"rules": {
		"comma-dangle": [2, "never"],
		"no-dupe-args": 2,
		"no-dupe-keys": 2,
		"no-duplicate-case": 2,
		"no-empty-character-class": 2,
		"no-ex-assign": 2,
		"no-extra-boolean-cast": 2,
		"no-extra-parens": 2,
		"no-extra-semi": 2,
		"no-func-assign": 2,
		"no-inner-declarations": 2,
		"no-invalid-regexp": 2,
		"no-irregular-whitespace": 2,
		"no-negated-in-lhs": 2,
		"no-obj-calls": 2,
		"no-regex-spaces": 2,
		"no-reserved-keys": 2,
		"no-empty": 2,
		"no-debugger": 2,
		"no-unreachable": 2,
		"use-isnan": 2,
		"valid-jsdoc": 2,
		"valid-typeof": 2,
		"no-caller": 2,
		"no-else-return": 2,
		"no-eq-null": 2,
		"no-eval": 2,
		"no-extend-native": 2,
		"no-extra-bind": 2,
		"no-fallthrough": 2,
		"no-floating-decimal": 2,
		"no-implied-eval": 2,
		"no-iterator": 2,
		"no-labels": 2,
		"no-lone-blocks": 2,
		"no-multi-spaces": 2,
		"no-multi-str": 2,
		"no-native-reassign": 2,
		"no-new-func": 2,
		"no-new-wrappers": 2,
		"no-octal-escape": 2,
		"no-param-reassign": 1,
		"no-proto": 2,
		"no-redeclare": 2,
		"no-return-assign": 2,
		"no-script-url": 2,
		"no-self-compare": 2,
		"no-throw-literal": 2,
		"no-void": 2,
		"no-with": 2,
		"wrap-iife": 2,
		"yoda": [2, "never", {
			"exceptRange": true,
			"onlyEquality": true
        }],

		"eqeqeq": 2,
		"no-catch-shadow": 2,
		"no-label-var": 2,
		"no-delete-var": 2,
		"no-shadow-restricted-names": 2,
		"no-shadow": 2,
		"no-undef-init": 2,
		"no-undef": 2,
		"no-undefined": 2,
		"no-use-before-define": 2,

		"brace-style": [2, "1tbs", {
			"allowSingleLine": true
		}],
		"camelcase": [2, {
			"properties": "always"
		}],
		"comma-spacing": [2, {
			"before": false,
			"after": true
		}],
		"comma-style": [2, "last"],
		"computed-property-spacing": [2, "never"],
		"eol-last": 2,
		"indent": 2,
		"new-parens": 2,
		"no-continue": 2,
		"no-lonely-if": 2,
		"no-mixed-spaces-and-tabs": 2,
		"no-multiple-empty-lines": 2,
		"no-nested-ternary": 2,
		"no-new-object": 2,
		"no-trailing-spaces": 2,
		"no-underscore-dangle": 2,
		"no-unneeded-ternary": 2,
		"object-curly-spacing": [2, "always"],
		"quotes": [1, "single"],
		"space-after-keywords": [2, "always"],
		"space-before-blocks": [2, "always"],
		"space-before-function-paren": [2, "always"],
		"space-infix-ops": [2, {
			"int32Hint": false
		}],
		"space-return-throw-case": 2,
		"space-unary-ops": 2,
		"wrap-regex": 2,

		"no-sparse-arrays": 1,
		"no-console": 0,
		"no-alert": 1,
		"no-constant-condition": 1,
		"no-unused-vars": 1,

		"vars-on-top": 0,
		"no-loop-func": 0,
		"strict": 0,
		"no-path-concat": 0
	}
}
